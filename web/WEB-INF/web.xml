<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Registrar</servlet-name>
        <servlet-class>Controladores.Registrar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Cursos</servlet-name>
        <servlet-class>Controladores.Cursos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Cursos2</servlet-name>
        <servlet-class>Controladores.Cursos2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Salir</servlet-name>
        <servlet-class>Controladores.Salir</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Imagen</servlet-name>
        <servlet-class>Controladores.Imagen</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Votar</servlet-name>
        <servlet-class>Controladores.Votar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Votos</servlet-name>
        <servlet-class>Controladores.Votos</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Registrar</servlet-name>
        <url-pattern>/Registrar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Cursos</servlet-name>
        <url-pattern>/Cursos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Cursos2</servlet-name>
        <url-pattern>/Cursos2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Salir</servlet-name>
        <url-pattern>/Salir</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Imagen</servlet-name>
        <url-pattern>/Imagen</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Votar</servlet-name>
        <url-pattern>/Votar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Votos</servlet-name>
        <url-pattern>/Votos</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            50
        </session-timeout>
    </session-config>
</web-app>
